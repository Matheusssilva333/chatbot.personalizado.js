{
  "name": "bot-luana",
  "version": "1.0.0",
  "description": "Bot Discord Luana - Assistente de IA com personalidade intelectual e reflexiva",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:all": "concurrently -n bot,ui -c blue,green \"nodemon src/index.js\" \"vite\"",
    "ui": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run --reporter=verbose",
    "deploy:commands": "node src/deploy-commands.js"
  },
  "keywords": [
    "bot",
    "ai",
    "discord",
    "luana",
    "minecraft",
    "xadrez",
    "filosofia"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "lucide-react": "^0.548.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  }
}
